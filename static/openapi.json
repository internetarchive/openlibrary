    "/works/{work_id}/bookshelves": {
      "get": {
        "summary": "Get bookshelf counts for a work",
        "description": "Returns the number of users who have placed a work on each reading log shelf (Want to Read, Currently Reading, Already Read).",
        "operationId": "get_work_bookshelves",
        "tags": ["reading log"],
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Work Id",
              "type": "string",
              "example": "OL123W"
            },
            "name": "work_id",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "counts": {
                      "type": "object",
                      "properties": {
                        "want_to_read": { "type": "integer" },
                        "currently_reading": { "type": "integer" },
                        "already_read": { "type": "integer" }
                      }
                    }
                  }
                }
              }
            }
          },
          "422": {
            "description": "Validation Error"
          }
        }
      },
      "post": {
        "summary": "Add or remove a book from a reading log shelf",
        "description": "Add or remove a work from the authenticated user's reading log shelf. Requires authentication via session cookie.",
        "operationId": "add_to_bookshelf",
        "tags": ["reading log"],
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Work Id",
              "type": "string",
              "example": "OL123W"
            },
            "name": "work_id",
            "in": "path"
          },
          {
            "required": true,
            "schema": {
              "title": "Bookshelf Id",
              "type": "integer",
              "description": "1 = Want to Read, 2 = Currently Reading, 3 = Already Read, -1 = Remove from shelf"
            },
            "name": "bookshelf_id",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Action",
              "type": "string",
              "default": "add"
            },
            "name": "action",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Edition Id",
              "type": "string"
            },
            "name": "edition_id",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "bookshelves_affected": {
                      "type": "array",
                      "items": { "type": "integer" }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized - User not logged in"
          },
          "422": {
            "description": "Validation Error"
          }
        }
      }
    },
    "/works/{work_id}/check-ins": {
      "post": {
        "summary": "Create or update a reading check-in event",
        "description": "Record reading progress (start reading, update progress, or finish reading). Requires authentication.",
        "operationId": "create_check_in",
        "tags": ["reading log"],
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Work Id",
              "type": "string",
              "example": "OL123W"
            },
            "name": "work_id",
            "in": "path"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["event_type", "year"],
                "properties": {
                  "event_type": {
                    "type": "integer",
                    "description": "1 = Started reading, 2 = Updated progress, 3 = Finished"
                  },
                  "year": { "type": "integer" },
                  "month": { "type": "integer" },
                  "day": { "type": "integer" },
                  "edition_key": { "type": "string" },
                  "event_id": {
                    "type": "integer",
                    "description": "If provided, updates existing event"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": { "type": "string" },
                    "id": { "type": "integer" }
                  }
                }
              }
            }
          },
          "401": { "description": "Unauthorized" },
          "403": { "description": "Forbidden" },
          "404": { "description": "Not Found" },
          "422": { "description": "Validation Error" }
        }
      },
      "delete": {
        "summary": "Delete a check-in event",
        "description": "Remove a reading check-in event. Requires authentication.",
        "operationId": "delete_check_in",
        "tags": ["reading log"],
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Work Id",
              "type": "string"
            },
            "name": "work_id",
            "in": "path"
          },
          {
            "required": true,
            "schema": {
              "title": "Event Id",
              "type": "integer"
            },
            "name": "event_id",
            "in": "query"
          }
        ],
        "responses": {
          "200": { "description": "Successful Response" },
          "401": { "description": "Unauthorized" },
          "403": { "description": "Forbidden" }
        }
      }
    },
    "/people/{username}/books/{shelf}": {
      "get": {
        "summary": "Get public reading log for a user",
        "description": "Returns a user's public reading log for a specific shelf. Only returns data if the user has made their reading log public.",
        "operationId": "get_public_reading_log",
        "tags": ["reading log"],
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Username",
              "type": "string"
            },
            "name": "username",
            "in": "path"
          },
          {
            "required": true,
            "schema": {
              "title": "Shelf",
              "type": "string",
              "enum": ["want-to-read", "currently-reading", "already-read"]
            },
            "name": "shelf",
            "in": "path"
          },
          {
            "required": false,
            "schema": {
              "title": "Page",
              "type": "integer",
              "default": 1
            },
            "name": "page",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Limit",
              "type": "integer",
              "default": 100,
              "maximum": 100
            },
            "name": "limit",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Query",
              "type": "string",
              "description": "Search query - minimum 3 characters"
            },
            "name": "q",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "page": { "type": "integer" },
                    "numFound": { "type": "integer" },
                    "reading_log_entries": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "work": { "type": "object" },
                          "logged_edition": { "type": "string" },
                          "logged_date": { "type": "string" }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "404": { "description": "User or shelf not found" }
        }
      }
    },
